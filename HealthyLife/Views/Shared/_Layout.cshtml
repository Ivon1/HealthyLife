<!DOCTYPE html>
<html lang="uk">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href='https://fonts.googleapis.com/css?family=Yanone Kaffeesatz' rel='stylesheet'>
    <title>@ViewData["Title"] - HealthyLife</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" 
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/swiper.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/catalog.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/course.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/adaptive.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/HealthyLife.styles.css" asp-append-version="true" />
    <link rel="icon" href="~/img/icons/logo.png"/>
</head>
    <body>
        <div id="pre-header" style="color:#FFFFFF; height:50px;" class="center-items-inside">
            <h4>Інвестуй у себе, інвестуй у здоров'я</h4>
        </div>

        <partial name="_PartialNavigation" />

        <main class="main" role="main">
            @RenderBody()
        </main>

        <partial name="_PartialFooter" />

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" 
            integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous">
        </script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/cart.js" asp-append-version="true"></script>
        <script src="~/js/wishlist.js" asp-append-version="true"></script>
        <script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
